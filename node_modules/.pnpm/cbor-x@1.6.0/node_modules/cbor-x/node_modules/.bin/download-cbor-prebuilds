#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ericvincent/Projects/private/bsky-reply-bots/node_modules/.pnpm/cbor-extract@2.2.0/node_modules/cbor-extract/bin/node_modules:/Users/ericvincent/Projects/private/bsky-reply-bots/node_modules/.pnpm/cbor-extract@2.2.0/node_modules/cbor-extract/node_modules:/Users/ericvincent/Projects/private/bsky-reply-bots/node_modules/.pnpm/cbor-extract@2.2.0/node_modules:/Users/ericvincent/Projects/private/bsky-reply-bots/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ericvincent/Projects/private/bsky-reply-bots/node_modules/.pnpm/cbor-extract@2.2.0/node_modules/cbor-extract/bin/node_modules:/Users/ericvincent/Projects/private/bsky-reply-bots/node_modules/.pnpm/cbor-extract@2.2.0/node_modules/cbor-extract/node_modules:/Users/ericvincent/Projects/private/bsky-reply-bots/node_modules/.pnpm/cbor-extract@2.2.0/node_modules:/Users/ericvincent/Projects/private/bsky-reply-bots/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cbor-extract@2.2.0/node_modules/cbor-extract/bin/download-prebuilds.js" "$@"
else
  exec node  "$basedir/../../../../../cbor-extract@2.2.0/node_modules/cbor-extract/bin/download-prebuilds.js" "$@"
fi
